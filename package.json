{
  "name": "gaming",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node --env-file=.env dist/server.js",
    "dev": "tsx watch --env-file=.env.dev src/server.ts",
    "prebuild": "pnpm run db:generate",
    "build": "rm -rf dist && tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json && tsc-esm-fix dist",
    "db:migrate": "prisma migrate dev",
    "db:migrate:create": "prisma migrate dev --create-only --skip-generate",
    "db:generate": "prisma generate --no-engine",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "deploy": "./deploy"
  },
  "keywords": [
    "gaming",
    "oRPC",
    "RPC"
  ],
  "author": "Ashik Sarkar <ashiksarkar.s18@gmail.com>",
  "license": "ISC",
  "packageManager": "pnpm@10.22.0",
  "dependencies": {
    "@orpc/openapi": "^1.11.3",
    "@orpc/server": "^1.11.3",
    "@orpc/zod": "^1.11.3",
    "@prisma/client": "^6.19.0",
    "express": "^5.1.0",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "tsc-alias": "^1.8.16",
    "tsc-esm-fix": "^3.1.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
