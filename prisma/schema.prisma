generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Auth {
  id         String  @id
  email      String  @unique
  password   String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @default(now())
}